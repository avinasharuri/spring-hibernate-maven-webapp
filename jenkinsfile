 pipeline {
        agent any
  stages {
        stage(scm) {
          steps {
                git 'https://github.com/avinasharuri/spring-hibernate-maven-webapp.git'
          }
        }

        stage (compile) {
          steps {
                sh 'mvn compile'
		}
             		   }
	  steps {
		sh 'mvn test'	        
                }
        }
}              
} 
